<?xml version="1.0"?>
<package >
  <metadata>
    <id>GiphyPropertyEditor</id>
    <version>1.0.5</version>
    <title>Giphy Property Editor</title>
    <authors>Paul Seal</authors>
    <owners>Paul Seal</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/prjseal/Giphy-Property-Editor</projectUrl>
    <iconUrl>https://github.com/prjseal/Giphy-Property-Editor/blob/master/images/logo.jpg?raw=true</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A property editor for Umbraco which looks like the media picker, but lets you search for animated gifs, powered by the Giphy api.</description>
    <releaseNotes>
      - removed dependencies
      - put giphy api key in app settings using the key name giphyApiKey
      - added grid editor
      - added property value converter
    </releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>Umbraco Property Editor Media Giphy Gif Search</tags>
  </metadata>
  <files>
    <file src="App_Plugins\GiphyPropertyEditor\**\*.*" target="content\App_Plugins\GiphyPropertyEditor" />
    <file src="Views\Partials\Grid\Editors\Giphy.cshtml" target="content\Views\Partials\Grid\Editors\Giphy.cshtml" />
  </files>
</package>